{
  "name": "itpe-obsidian-plugin",
  "version": "1.0.0",
  "description": "ITPE Topic Enhancement System integration for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "itpe",
    "topic-enhancement"
  ],
  "author": "turtlesoup0",
  "license": "MIT",
  "dependencies": {
    "obsidian": "latest",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.0.0",
    "@vitest/ui": "^2.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.0",
    "jsdom": "^28.0.0",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0"
  }
}
